                <xs:element name="limit_group" type="xs:string" />
                <xs:element name="limit_kind" type="xs:int" minOccurs="0" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="limits">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="limitcard_name" type="xs:string" />
                <xs:element name="limit_group" type="xs:string" />
                <xs:element name="compoundinfo_name" type="xs:string" />
