        <xs:field xpath="limit_group" />
      </xs:unique>
      <xs:unique name="Constraint2">
        <xs:selector xpath=".//limitcard" />
        <xs:field xpath="limitcard_name" />
      </xs:unique>
      <xs:unique name="limits_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
        <xs:selector xpath=".//limits" />
        <xs:field xpath="limitcard_name" />
        <xs:field xpath="limit_group" />
