      <xs:unique name="localized_limitcard_names_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
        <xs:selector xpath=".//localized_limitcard_names" />
        <xs:field xpath="limitcard_name" />
        <xs:field xpath="localized_standard" />
      </xs:unique>
      <xs:keyref name="Relation_limitcards_localnames" refer="Constraint2">
        <xs:selector xpath=".//localized_limitcard_names" />
        <xs:field xpath="limitcard_name" />
      </xs:keyref>
      <xs:keyref name="Relation_limitcards_limits" refer="Constraint1">
        <xs:selector xpath=".//limits" />
        <xs:field xpath="limitcard_name" />
        <xs:field xpath="limit_group" />
